{
    "version": 2,
    "builds": [
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        },
        {
            "src": "api/get-api-key.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/get-api-key",
            "dest": "/api/get-api-key.js"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}
