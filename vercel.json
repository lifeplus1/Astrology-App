{
  "version": 2,
  "builds": [
    {
      "src": "app/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/app/(.*\\.js)",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/app/$1"
    },
    {
      "src": "/assets/images/(.*\\.ico)",
      "headers": { "Content-Type": "image/x-icon" },
      "dest": "/app/assets/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app/index.html"
    }
  ]
}
